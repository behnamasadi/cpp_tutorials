{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Current C++ File",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/${fileBasenameNoExtension}",
            "args": ["foo", "fee", "bar"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
